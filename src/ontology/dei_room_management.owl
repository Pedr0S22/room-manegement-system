          <owl:onProperty>
            <rdf:Description>
              <owl:inverseOf rdf:resource="#BookedInRoom"/>
            </rdf:Description>
          </owl:onProperty>
          <owl:someValuesFrom rdf:resource="#RoomBooking"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
